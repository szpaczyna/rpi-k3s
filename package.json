{
  "name": "rpi-k3s",
  "version": "0.0.0",
  "description": "Szpacze Gniazdo k3s",
  "main": "index.js",
  "dependencies": {
    "semantic-release": "^17.4.2"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "git log --pretty=format:"* [%s](https://github.com/szpaczyna/rpi-k3s/commit/%H)" $(git tag --sort=-committerdate | head -1)...$(git tag --sort=-committerdate | head -2 | awk '{split($0, tags, "\n")} END {print tags[1]}') > RELEASE.md; standard-version -i RELEASE.md --skip.commit --header "# Changelog for Szpacze Gniazdo K3s",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szpaczyna/rpi-k3s.git"
  },
  "author": "Michal 'shpaq' Laszuk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/szpaczyna/rpi-k3s/issues"
  },
  "homepage": "https://github.com/szpaczyna/rpi-k3s#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
