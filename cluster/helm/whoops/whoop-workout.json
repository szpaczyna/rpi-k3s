          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  zone, time_ms / 1000 as time_s\nFROM \n  whoop_workout,\n  LATERAL (\n    VALUES \n      ('Zone 0 (50-60%)', zone_zero_milli),\n      ('Zone 1 (60-70%)', zone_one_milli),\n      ('Zone 2 (70-80%)', zone_two_milli),\n      ('Zone 3 (80-90%)', zone_three_milli),\n      ('Zone 4 (90-100%)', zone_four_milli),\n      ('Zone 5 (MAX)', zone_five_milli)\n  ) AS t(zone, time_ms)\nWHERE \n  id = '${workout_id}' AND time_ms IS NOT NULL AND time_ms > 0;",
          "refId": "A"
        }
      ],
      "title": "Time in Heart Rate Zones",
